a = int(input("Enter first Number : "))
b = int(input("Enter Second Numbrer : "))
c = int(input("Enter Third Number : "))
if a>b and a>c:
    print("Greatest Number is ",a)
if b>a and b>c:
    print("Greatest Number is ",b)
else:
    print("Greatest Number is ",c)
