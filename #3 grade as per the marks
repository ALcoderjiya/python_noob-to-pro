per = float(input("Enter Percentage Marks : "))
if per > 100:
    print("Invalid Input")
else:
    if per >= 90:
        print("Grade is A+")
    if per >= 80 and per < 90:
        print("Grade is A")
    if per >= 60 and per < 80:
        print("Grade is B")
    if per >= 50 and per < 60:
        print("Grade is C")
    if per < 50:
        print("Grade is D")
